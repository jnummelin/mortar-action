apiVersion: extensions/v1beta1
kind: Ingress
metadata:
  namespace: <%= var.namespace %>
  name: hello
  annotations:
    nginx.ingress.kubernetes.io/rewrite-target: /
spec:
  rules:
  - host: <%= var.host %>
    http:
      paths:
      - path: /
        backend:
          serviceName: hello
          servicePort: 9292